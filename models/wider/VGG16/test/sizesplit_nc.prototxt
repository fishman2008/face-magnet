#========= RPN ============

layer {
  name: "rpn_conv/3x3_s"
  type: "Convolution"
  bottom: "conv_height_2_s"
  top: "rpn/output3x3_s"
  param { lr_mult: 1.0 decay_mult: 1.0 }
  param { lr_mult: 2.0 decay_mult: 0 }
  convolution_param {
    num_output: 384
    kernel_size: 3 pad: 1 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

layer {
  name: "rpn_conv/5x5_s"
  type: "Convolution"
  bottom: "conv_height_2_s"
  top: "rpn/output5x5_s"
  param { lr_mult: 1.0 decay_mult: 1.0 }
  param { lr_mult: 2.0 decay_mult: 0 }
  convolution_param {
    num_output: 128
    kernel_size: 5 pad: 2 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}

layer{
  name: "rpn_concat/3x3and5x5_s"
  type: "Concat"
  bottom: "rpn/output3x3_s"
  bottom: "rpn/output5x5_s"
  top: "rpn/output_s"
  concat_param{
      axis: 1
  }
}

layer {
  name: "brpn_cls_score_s"
  type: "Convolution"
  bottom: "rpn/output_s"
  top: "rpn_cls_score_s"
  param { lr_mult: 1.0 decay_mult: 1.0 }
  param { lr_mult: 2.0 decay_mult: 0 }
  convolution_param {
    num_output: 12   # 2(bg/fg) * 21(anchors)
    kernel_size: 1 pad: 0 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name: "brpn_bbox_pred_s"
  type: "Convolution"
  bottom: "rpn/output_s"
  top: "rpn_bbox_pred_s"
  param { lr_mult: 1.0 decay_mult: 1.0 }
  param { lr_mult: 2.0 decay_mult: 0 }
  convolution_param {
    num_output: 24   # 4 * 21(anchors)
    kernel_size: 1 pad: 0 stride: 1
    weight_filler { type: "gaussian" std: 0.01 }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
   bottom: "rpn_cls_score_s"
   top: "rpn_cls_score_reshape_s"
   name: "rpn_cls_score_reshape_s"
   type: "Reshape"
   reshape_param { shape { dim: 0 dim: 2 dim: -1 dim: 0 } }
}
#============== ROI Proposal ===============
layer {
  name: "rpn_cls_prob_s"
  type: "Softmax"
  bottom: "rpn_cls_score_reshape_s"
  top: "rpn_cls_prob_s"
}
layer {
  name: 'rpn_cls_prob_reshape_s'
  type: 'Reshape'
  bottom: 'rpn_cls_prob_s'
  top: 'rpn_cls_prob_reshape_s'
  reshape_param { shape { dim: 0 dim:12  dim: -1 dim: 0 } }
}
layer {
  name: 'proposal'
  type: 'Python'
  bottom: 'rpn_cls_prob_reshape_s'
  bottom: 'rpn_bbox_pred_s'
  bottom: 'im_info'
  top: 'rois_s'
  top: 'rpn_scores_s'
  python_param {
    module: 'rpn.proposal_layer'
    layer: 'ProposalLayer'
    param_str: "{'feat_stride': 4,'scales': [0.3,0.5,1,1.5,2.5,3.2], 'ratios':[1,]}"
  }
}
#========= RCNN ============
### RCNN SMALL
#========= RCNN ============

layer {
  name: "roi_pool5_s"
  type: "ROIPooling"
  bottom: "conv_height_2_s"
  bottom: "rois_s"
  top: "pool5_s"
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.25 # 1/16
  }
}


#========= RCNN ============

layer {
  name: "copy_from_fc6"
  type: "InnerProduct"
  bottom: "pool5_s"
  top: "fc6_s"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu6_s"
  type: "ReLU"
  bottom: "fc6_s"
  top: "fc6_s"
}
layer {
  name: "drop6_s"
  type: "Dropout"
  bottom: "fc6_s"
  top: "fc6_s"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "copy_from_fc7"
  type: "InnerProduct"
  bottom: "fc6_s"
  top: "fc7_s"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu7_s"
  type: "ReLU"
  bottom: "fc7_s"
  top: "fc7_s"
}
layer {
  name: "drop7_s"
  type: "Dropout"
  bottom: "fc7_s"
  top: "fc7_s"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "cls_score_s"
  type: "InnerProduct"
  bottom: "fc7_s"
  top: "cls_score_s"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}
layer {
  name: "bbox_pred_s"
  type: "InnerProduct"
  bottom: "fc7_s"
  top: "bbox_pred_s"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 8
    weight_filler {
      type: "gaussian"
      std: 0.001
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}


