    pool: MAX
    kernel_size: 2
    stride: 2
  }
}


layer {
  name: "copy_from_conv3_1"
  type: "Convolution"
  bottom: "pool2"
  top: "conv3_1_s"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu3_1_s"
  type: "ReLU"
  bottom: "conv3_1_s"
  top: "conv3_1_s"
}
layer {
  name: "copy_from_conv3_2"
  type: "Convolution"
  bottom: "conv3_1_s"
  top: "conv3_2_s"
